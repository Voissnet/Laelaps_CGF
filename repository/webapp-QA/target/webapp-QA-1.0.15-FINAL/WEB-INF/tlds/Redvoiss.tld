<?xml version="1.0" encoding="UTF-8"?>
<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>submit</shortname>
	<info>Sample taglib for submit operation</info>
	<uri>http://www.redvoiss.net/jsp/taglib/submit</uri>
	<tag>
		<name>submit</name>
		<tagclass>net.redvoiss.jsp.custom.taglib.SubmitTagHandler</tagclass>
		<info>Submit once and disable button</info>
		<attribute>
			<name>input</name>
			<required>true</required>
		</attribute>
	</tag>
</taglib>
